{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      // "@typescript-eslint/no-unused-vars": ["error"],
      "@typescript-eslint/default-param-last": ["error"]
    }
  }
  